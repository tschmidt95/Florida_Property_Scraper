      "label": "Start: Backend Only (8000)",
      "type": "shell",
      "command": "bash",
      "args": [
        "-lc",
        "set -euo pipefail; source .venv/bin/activate; export LEADS_SQLITE_PATH=/workspaces/Florida_Property_Scraper/leads.sqlite; export PA_DB=\"${PA_DB:-$LEADS_SQLITE_PATH}\"; export FPS_USE_FDOR_CENTROIDS=\"${FPS_USE_FDOR_CENTROIDS:-1}\"; : > .uvicorn_8000.log; nohup python -m uvicorn florida_property_scraper.api.app:app --host 0.0.0.0 --port 8000 > .uvicorn_8000.log 2>&1 & echo $! > .uvicorn_8000.pid; sleep 0.2; kill -0 $(cat .uvicorn_8000.pid)"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "isBackground": true,
      "problemMatcher": [],
      "presentation": {
        "reveal": "always",
        "panel": "dedicated",
        "clear": false
      }
    },
    {
      "label": "Stop: Kill 8000",
      "type": "shell",
      "command": "bash",
      "args": [
        "-lc",
        "if [ -f .uvicorn_8000.pid ]; then pid=$(cat .uvicorn_8000.pid 2>/dev/null || true); if [ -n \"$pid\" ]; then kill -TERM \"$pid\" 2>/dev/null || true; fi; fi"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
