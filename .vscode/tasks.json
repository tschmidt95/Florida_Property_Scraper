      "type": "shell",
      "command": "bash",
      "args": [
        "-lc",
        "bash scripts/up.sh"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "isBackground": true,
      "problemMatcher": [],
      "presentation": {
        "reveal": "always",
        "panel": "dedicated",
        "clear": false
      }
    },
